                                <th>Contact</th>
                                <th>Website</th>
                                <th>Hours</th>
                                <th>Perks</th>
                                <th>Type</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each data}}
                                <tr id={{resourceId}}>
                                    <td>{{location}}</td>
                                    <td>{{organization}}</td>
                                    <td>{{description}}</td>
                                    <td>{{keywords}}</td>
                                    <td>{{phoneNumber}}</td>
                                    <td>{{website}}</td>
                                    <td>{{hours}}</td>
                                    <td>{{perks}}</td>
                                    <td>{{type}}</td>
                                </tr>
                            {{/each}}
